import random
a=int(input('kolvo gl?'))
Sogl=['Й','Ц','К','Н','Г','Ш','Щ','З','Х','Ф','В','П','Р','Л','Д','Ж','Ч','С','М','Т','Б','й','ц','к','н','г','ш','щ','з','х','ф','в','п','л','д','ж','ч','с','м','т','б']
gl=['а','е','и']
word=[]
b=''
for i in range(a):
    word.append(random.choice(Sogl))
    word.append(gl[i])
for i in range(len(word)):
    b+=word[i]
print(b)
